#! @SHELL@

if test -z "$srcdir"; then
  srcdir=.
fi
. ${srcdir}/defs

cp ${top_builddir}/src/iit_store iit_store
chmod +x iit_store
cp ${top_builddir}/src/iit_get iit_get
chmod +x iit_get
cp ${top_builddir}/src/iit_dump iit_dump
chmod +x iit_dump


./iit_store -o iittest ${srcdir}/fa.iittest
#diff iittest.iit ${srcdir}/iittest.iit.ok

./iit_get iittest.iit 20000 > iit_get.out
diff iit_get.out ${srcdir}/iit_get.out.ok

./iit_dump iittest.iit > iit_dump.out
diff iit_dump.out ${srcdir}/fa.iittest

